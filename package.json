{
  "publisher": "rcoopr",
  "name": "duplicat",
  "displayName": "DupliCat",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.7.1",
  "description": "Duplicate files with ease",
  "author": "Ross Cooper",
  "license": "MIT",
  "funding": "https://github.com/sponsors/rcoopr",
  "homepage": "https://github.com/rcoopr/duplicat#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/rcoopr/duplicat"
  },
  "bugs": {
    "url": "https://github.com/rcoopr/duplicat/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "LICENSE.md",
    "dist/*",
    "res/*"
  ],
  "engines": {
    "vscode": "^1.92.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [],
    "configuration": {
      "type": "object",
      "properties": {
        "duplicat.openDuplicatedFile": {
          "type": "boolean",
          "default": true,
          "description": "Open the duplicated file after duplication"
        }
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "prepare": "nr update",
    "update": "vscode-ext-gen --output src/generated/meta.ts --scope duplicat",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@antfu/ni": "^0.22.4",
    "@types/node": "^22.13.4",
    "@types/vscode": "^1.97.0",
    "@vscode/vsce": "^2.32.0",
    "bumpp": "^9.11.1",
    "eslint": "^9.20.1",
    "eslint-plugin-format": "^0.1.3",
    "esno": "^4.8.0",
    "pnpm": "^9.15.5",
    "reactive-vscode": "0.2.10",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vite": "^5.4.14",
    "vitest": "^2.1.9",
    "vscode-ext-gen": "^0.3.7"
  }
}
